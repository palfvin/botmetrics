<% provide(:title, 'Sign up') %>
<h1>What do you want to chart?</h1>

<div class="row">
  <div class="span6 offset3">
    <%= bootstrap_form_for(@chart) do |f| %>
      <%= render 'fields', f: f %>
      <%= f.submit "Create my chart", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
