<% provide(:title, @chart.id) %>
<div id='container' style="width:45%; height:200px;"></div>
<script>
$(document).ready(function () {
  $('#container').highcharts( <%= @chart.javascript.gsub(/"([^(")]+)":/, '\1:').html_safe %> );
});
</script>
<div class="row">
  <aside class="span4">
    <%= link_to 'Edit', edit_chart_path(@chart.id) %>
  </aside>
</div>